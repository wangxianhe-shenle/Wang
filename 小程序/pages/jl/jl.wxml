<!--pages/jl/jl.wxml-->
<view>
  <view class='img'>
    <image src='https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1560306564925&di=a6f19556460ddac536b405229dd2d3b7&imgtype=0&src=http%3A%2F%2Fbpic.588ku.com%2Fback_pic%2F03%2F81%2F25%2F7657c53870ddf96.jpg'></image>
  </view>
  <view class='zl'>
    <view class='weui-cell-third'>
      <view class="page__title">工作经历</view>
      <block wx:for="{{axis}}" wx:key="*this">
        <view class='weui-cell-list'>
          <view class='weui-cell-circle'>
            <view class='weui-cell-time'>{{item.time}}</view>
          </view>
          <view class='weui-cell-line'>
            <view class='weui-cell-name'>{{item.name}}</view>
            <view class='weui-cell-event'>{{item.event}}</view>
          </view>
        </view>
      </block>
      <view class='weui-cell-list'>
        <view class='weui-cell-circle'></view>
      </view>
      <view class="page__title">教育经历</view>
      <block wx:for="{{axis1}}" wx:key="*this">
        <view class='weui-cell-list'>
          <view class='weui-cell-circle'>
            <view class='weui-cell-time'>{{item.time}}</view>
          </view>
          <view class='weui-cell-line'>
            <view class='weui-cell-name'>{{item.name}}</view>
            <view class='weui-cell-event'>{{item.event}}</view>
          </view>
        </view>
      </block>
    </view>
    <view class='weui-cell-list'>
      <view class='weui-cell-circle'></view>
    </view>
  </view>
</view>