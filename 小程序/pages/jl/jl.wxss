.img {
  width: 100%;
  height: 380rpx;
}

.img image {
  width: 100%;
  height: 380rpx;
}

.zl {
  position: absolute;
  margin-top: -40rpx;
  background: #fff;
  box-shadow: 0px 2px 4px 2px #ddd;
  border-width: 0rpx;
  width: 90%;
  margin-left: 5%;
}

.weui-cell-third {
  background: #fff;
}

.weui-cell-list {
  background: #fff;
  margin-left:100rpx;

}

.weui-cell-line {
  margin-left: 5px;
  height: 180rpx;
  border-left: 3rpx solid #ddd;
  background: #fff;
}

.weui-cell-circle {
  border: 1px solid #000;
  border-radius: 50%;
  width: 10px;
  height: 10px;
  border-color: #ddd;
}

.weui-cell-time {
  font-size: 20rpx;
  padding-left: 15px;
  width: 400rpx;
}

.weui-cell-event {
  padding-top: 15px;
}

.weui-cell-name {
  margin-left: 20rpx;
  font-size: 14px;
  padding-top: 20px;
  height: 23px;
}
.page__title{
  width: 100%;
  height: 40px;
  line-height: 40px;
  margin-top: 40rpx;
  margin-left: 20rpx;
}
